{
  "name": "redup",
  "version": "1.0.0",
  "description": "Redup â€” Post Smarter. Rank Higher.",
  "private": true,
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --watch server.js --watch src --ext js,ejs --exec node server.js",
    "build:css": "tailwindcss -i ./src/public/css/input.css -o ./src/public/css/tailwind.css --minify",
    "watch:css": "tailwindcss -i ./src/public/css/input.css -o ./src/public/css/tailwind.css --watch",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "worker": "node src/jobs/worker.js"
  },
  "nodemonConfig": {
    "ignore": [
      "src/public/css/tailwind.css"
    ]
  },
  "keywords": [
    "reddit",
    "scheduler",
    "analytics",
    "nodejs",
    "express",
    "tailwindcss",
    "ejs",
    "saas"
  ],
  "author": "Redup Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.17"
  },
  "dependencies": {
    "cheerio": "^1.1.2",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-ejs-layouts": "^2.5.1",
    "express-session": "^1.18.2",
    "fetch-blob": "^3.2.0",
    "mysql2": "^3.9.7",
    "openai": "^6.5.0",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "passport-reddit": "^1.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "nodemon": "^3.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18"
  }
}
